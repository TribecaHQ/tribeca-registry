{
  "name": "@tribecahq/registry",
  "description": "Registry of Tribeca DAOs.",
  "version": "0.2.4",
  "keywords": [
    "solana",
    "tribeca",
    "governance"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "repository": "git@github.com:TribecaHQ/tribeca-registry.git",
  "author": "Tribeca Team <team@tribeca.so>",
  "bugs": {
    "url": "https://github.com/TribecaHQ/tribeca-registry/issues",
    "email": "team@tribeca.so"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://tribeca.so",
  "license": "Apache-2.0",
  "packageManager": "yarn@4.0.0-rc.22",
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@saberhq/eslint-config": "^2.1.0",
    "@saberhq/solana-contrib": "^1.14.9",
    "@saberhq/token-utils": "^1.14.9",
    "@saberhq/tsconfig": "^2.1.0",
    "@solana/web3.js": "^1.63.1",
    "@types/bn.js": "^5.1.1",
    "@types/eslint": "^8.4.6",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.8.2",
    "@types/prettier": "^2.7.1",
    "bn.js": "^5.2.1",
    "eslint": "^8.24.0",
    "eslint-import-resolver-node": "^0.3.6",
    "fast-json-stable-stringify": "^2.1.0",
    "husky": "^8.0.1",
    "jsbi": "^4.3.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "build": "tsc -P tsconfig.build.json && tsc -P tsconfig.esm.json",
    "generate": "mkdir -p data/registry && cp -R static/* data/ && npm run build && npm run generate:solana-token-list && npm run generate:governor-metas",
    "generate:solana-token-list": "node dist/cjs/scripts/generateTokenList.js",
    "generate:governor-metas": "node dist/cjs/scripts/generateGovernorMetas.js",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix --cache",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{md,json,js,yml,yaml,html}": "prettier --write"
  },
  "dependencies": {
    "cross-fetch": "^3.1.5",
    "lodash": "^4.17.21",
    "tiny-invariant": "^1.3.1",
    "toml": "^3.0.0",
    "tslib": "^2.4.0"
  },
  "files": [
    "src",
    "dist"
  ]
}
